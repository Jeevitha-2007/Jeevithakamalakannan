# priblem link https://www.geeksforgeeks.org/problems/triangle-number/1?page=1&category=pattern-printing&sortBy=submissions
#User function Template for python3

class Solution:
    def printTriangle(self, N):
        for a in range(1,N+1):
            for b in range(1,a+1):
                print(b,end=" ")
            print()
